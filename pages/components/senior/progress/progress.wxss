/* component/senior/modal/modal.wxss */
.progress {
  width: 95%;
  margin: 20rpx;
}
.progress-box{
  width: 76%;
  height: 240px;
  background-color: #fff;
  position: absolute;
  top: 50%;
  margin-top: -120px;
  left: 12%;
  border-radius: 3px;
}
.mask{
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,.5);
  position: fixed;
  z-index: 999;
}