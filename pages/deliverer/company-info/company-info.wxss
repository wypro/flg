
view{
  box-sizing: unset;
  padding: 0;
  margin: 0;
}
.ms{
  width: 100%;
  line-height: 30px;
  padding-left: 25px;
  font-size: 12px;
  color: darkgray;
  padding-top: 10px;
  padding-bottom: 10px;
  background-color:  rgb(245, 245, 245);
}
.info{
  width: 100%;
  height: 100%;
  line-height: 40px;
  margin: 0 auto;
  font-size: 14px;
  color: black;
}
.info>view{
  width: 100%;
  flex-direction: row;
  display: flex;
  margin: 0 auto;
  border-bottom: 1px solid rgb(245, 245, 250);
}
.info>view>view{
  width: 55%;
  padding-left: 25px; 
}
.info>view>input{
  width: 38%;
  padding-top: 8px; 
  margin: 0;
  text-align: right;
  align-items: center;
  margin-right: 25px;
}
.info>view>label{
  width: 38%;
  margin: 0;
  text-align: right;
  color: darkgray;
}
.center-icon{
  width: 14px;
  height: 14px;
  margin: 0 5px;
  padding: 0;
}
.piker{
  color: black;
  margin-bottom: 4px;
}
.btn{
  width: 85%;
  margin: 0 auto;
  margin-top: 20px;
  margin-bottom: 50px;
  background-color: #2BC998;
  color: white;
  position: relative;
}
.logo{
  margin: 0 auto;
  width: 60px;
  height: 60px;
  overflow: hidden;

}
.logo image{
  width: 100%;
  height: 100%;
}
.modify-logo{
  font-size: 14px;
  margin: 0 auto;
  text-align: center;
  color: rgb(33, 223, 163);
  padding-bottom: 10px; 
  padding-top: 10px; 
}
.chooseImage{
  margin: 0 auto;
  width: 60px;
  height: 120px;
}
img {
  border: 0; vertical-align:middle;
  } 